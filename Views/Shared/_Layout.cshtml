@{ 
    Kelloggs.Models.ResponseData data = new Kelloggs.Models.ResponseData();
    if (Session["IsSuccess"] != null)
    {
        data = (Kelloggs.Models.ResponseData)Session["IsSuccess"];
    }
}
<!DOCTYPE html>
<html>
<head>
